<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-Language" content="en">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />

    <title>Demo powered by Openplata.com</title>
    <link rel="icon" href="~/Resources/openplata.ico" type="image/x-icon" />

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no" />
    <meta name="description" content="Seguridad de Openplata">
    <meta name="msapplication-tap-highlight" content="no">

    <script type="text/javascript" src="~/ViewsScripts/Views/LayoutView.js"></script>

    @Scripts.Render("~/AdminLTE/plugins/jquery/js")
    @Scripts.Render("~/AdminLTE/bootstrap/js")
    @Scripts.Render("~/AdminLTE/plugins/jquery-ui/js")
    @Scripts.Render("~/Notificaciones/Scripts")
    @Scripts.Render("~/Content/Openplata/Scripts")
    @Scripts.Render("~/AdminLTE/plugins/slimscroll/js")
    @Scripts.Render("~/AdminLTE/plugins/fastclick/js")
    @Scripts.Render("~/AdminLTE/plugins/timepicker/js")
    @Scripts.Render("~/AdminLTE/plugins/select2/4.0/js")
    @Scripts.Render("~/AdminLTE/dist/js")
    @Scripts.Render("~/Scripts/UnobtrusiveAjax")
    @Scripts.Render("~/Scripts/ValidateUnobtrusive")
    @Scripts.Render("~/AdminLTE/plugins/datepicker/js")
    @Scripts.Render("~/AdminLTE/bootstrap-dialog/Scripts")
    @Scripts.Render("~/AdminLTE/plugins/ckeditor/4.8.0")
    @Scripts.Render("~/AdminLTE/bootstrap-toggle/js")

    <link href="~/AdminLTE/OpenplataUI 1.0/OpenplataUI.css" rel="stylesheet">

    

</head>
<body>

    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header fixed-footer" id="appContainer">
        <div class="app-header header-shadow">
            <div class="app-header__mobile-menu" style="display:flex">
                <div>
                    <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav" onclick="_LayoutView.OnClickMenu(this);">
                        <span class="hamburger-box">
                            <span class="hamburger-inner"></span>
                        </span>
                    </button>
                </div>
            </div>

            <div class="app-header__logo">
                <div class="logo-src"></div>
            </div>

            <div class="app-header__menu">
                <span>
                    <button type="button" class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                        <span class="btn-icon-wrapper">
                            <i class="fa fa-ellipsis-v fa-w-6"></i>
                        </span>
                    </button>
                </span>
            </div>
            <div class="app-header__content">

                <div class="app-header-right">
                    <div class="header-btn-lg pr-0">
                        <div class="widget-content p-0">
                            <div class="widget-content-wrapper">
                                <div class="widget-content-left">
                                    <div class="btn-group">
                                        <a data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="p-0 btn">
                                            <img width="42" class="rounded-circle" src="AdminLTE/OpenplataUI 1.0/assets/images/avatars/1.jpg" alt="">
                                            <i class="fa fa-angle-down ml-2 opacity-8"></i>
                                        </a>
                                        <div tabindex="-1" role="menu" aria-hidden="true" class="dropdown-menu dropdown-menu-right">
                                            <button type="button" tabindex="0" class="dropdown-item">User Account</button>
                                            <button type="button" tabindex="0" class="dropdown-item">Settings</button>
                                            <h6 tabindex="-1" class="dropdown-header">Header</h6>
                                            <button type="button" tabindex="0" class="dropdown-item">Actions</button>
                                            <div tabindex="-1" class="dropdown-divider"></div>
                                            <button type="button" tabindex="0" class="dropdown-item"
                                                    onclick="AppOF_Operacion_Financiera.CloseSessionConfirmation();">
                                                Cerrar Sesión
                                            </button>
                                        </div>
                                    </div>
                                </div>
                                <div class="widget-content-left  ml-3 header-user-info">
                                    <div class="widget-heading">
                                        Alina Mclourd
                                    </div>
                                    <div class="widget-subheading">
                                        VP People Manager
                                    </div>
                                </div>
                                <div class="widget-content-right header-user-info ml-3">
                                    <button type="button" class="btn-shadow p-1 btn btn-primary btn-sm show-toastr-example">
                                        <i class="fa text-white fa-calendar pr-1 pl-1"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="app-main">
            <div class="app-sidebar sidebar-shadow" id="appSideBar">
                <div class="app-header__logo">
                    <div class="logo-src"></div>
                    <div class="header__pane ml-auto">
                        <div>
                            <button type="button" class="hamburger close-sidebar-btn hamburger--elastic" data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="app-header__menu">
                    <span>
                        <button type="button" class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
                            <span class="btn-icon-wrapper">
                                <i class="fa fa-ellipsis-v fa-w-6"></i>
                            </span>
                        </button>
                    </span>
                </div>

                <div class="scrollbar-sidebar">
                    <div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu">
                            <li class="app-sidebar__heading">Dashboards</li>
                            <li>
                                <a id="WapMaterial" href="~/DashboardAnalisis/WapAnalisis">
                                    <i class="metismenu-icon pe-7s-rocket"></i>
                                    Análisis
                                </a>
                            </li>

                            <li class="app-sidebar__heading">Maestros</li>
                            <li>
                                <a id="WapMaterial" href="~/MaestroMaterial/WapMaterial">
                                    <i class="metismenu-icon pe-7s-wallet"></i>
                                    Material
                                </a>
                            </li>


                            <li class="app-sidebar__heading">Seguridad</li>
                            <li>
                                <a href="#" onclick="AppOF_Operacion_Financiera.CloseSessionConfirmation();">
                                    <i class="metismenu-icon pe-7s-lock">
                                    </i>Cerrar Sesión
                                </a>
                            </li>

                        </ul>
                    </div>
                </div>
            </div>

            <div class="app-main__outer">
                <div class="app-main__inner">
                    @RenderBody()
                </div>

                <div class="app-wrapper-footer">

                </div>
            </div>
            <script src="http://maps.google.com/maps/api/js?sensor=true"></script>
        </div>
    </div>

    <!-- BEGIN Código de bloquedo de la página mientras carga -->
    <div id="divLayoutBlock" style="display:none">
        <div class="blockUI" style="display:none"></div>
        <div class="blockUI blockOverlay" style="display: none; border: none; margin: 0px; padding: 0px; width: 100%; height: 100%; top: 0px; left: 0px; position: fixed;"></div>
        <div class="blockUI undefined blockPage" style="position: fixed; opacity: 0.751133;">
            <div class="body-block-example-1 d-none" style="cursor: default;">
                <div class="loader bg-transparent no-shadow p-0">
                    <div class="ball-spin-fade-loader">
                        <div class="bg-white"></div>
                        <div class="bg-white"></div>
                        <div class="bg-white"></div>
                        <div class="bg-white"></div>
                        <div class="bg-white"></div>
                        <div class="bg-white"></div>
                        <div class="bg-white"></div>
                        <div class="bg-white"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- END Código de bloquedo de la página mientras carga -->

    <script type="text/javascript" src="~/AdminLTE/OpenplataUI 1.0/assets/scripts/OpenplataUI.js"></script>
    <script type="text/javascript" src="~/ViewsScripts/Util/FormUtil.js"></script>

</body>
</html>

